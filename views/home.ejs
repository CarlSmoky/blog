<%- include("./partials/header") %>
  <h1>Hello world</h1>
  <p>
    <%= homeStartingContent %>
  </p>
  <% 
    const length = 10;
    posts.forEach(e=> { %>
      <h1><%= e.title %></h1>
      <%
      const title = e.title;
      const displayContent = e.content.length < length ? e.body : e.content.substring(0, length) + "..." %>
      <p>
        <%= displayContent%>
        <a href="posts/<%=e._id %>">Read More</a>
      </p>
      
      <%}) %>

<%- include("./partials/footer") %>